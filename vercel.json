{
  "version": 2,
  "builds": [
    {
      "src": "Build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "^/(.*)$", "dest": "/$1" }
  ]
  ,
  "headers": [
    {
      "source": "/Build/(.*)\\.gz",
      "headers": [
        { "key": "Content-Encoding", "value": "gzip" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/Build/(.*)\\.br",
      "headers": [
        { "key": "Content-Encoding", "value": "br" },
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
